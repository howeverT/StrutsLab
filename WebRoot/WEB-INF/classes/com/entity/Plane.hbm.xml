<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.entity.Plane" table="PlaneList">
		<!-- 配置主键 -->
		<id name="id" column="id">
			<generator class="native" /> <!-- 配置的是自动增长标识列 -->
		</id>
		<property name="uname" column="Name"/>
		<property name="sex" column="Sex"/>
		<property name="scity" column="startCity"/>
		<property name="dcity" column="Distinction"/>
		<property name="date" column="Date"/>
		<property name="ident" column="Identification"/>
		<property name="utage" column="utage"/>
	</class>
</hibernate-mapping>